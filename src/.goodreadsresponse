  <link>https://www.goodreads.com/book/show/77203.The_Kite_Runner</link>
  <num_pages>371</num_pages>
  <format></format>
  <edition_information/>
  <publisher>Riverhead Books</publisher>
  <publication_day>27</publication_day>
  <publication_year>2004</publication_year>
  <publication_month>4</publication_month>
